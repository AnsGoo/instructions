{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Django: Runserver",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/packages/instructions/manage.py",
      "args": ["runserver"],
      "django": true,
      "justMyCode": true,
      "cwd": "${workspaceFolder}/packages/instructions"
    },
    {
      "name": "Django: Test",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/packages/ext_model_test/manage.py",
      "args": ["test", "content.tests"],
      "django": true,
      "justMyCode": true,
      "cwd": "${workspaceFolder}/packages/ext_model_test"
    },
    {
      "name": "Django: Shell",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/packages/instructions/manage.py",
      "args": ["shell"],
      "django": true,
      "justMyCode": true,
      "cwd": "${workspaceFolder}/packages/instructions"
    }
  ]
}
